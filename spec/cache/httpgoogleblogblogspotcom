    //<![CDATA[

function validateAmount(amount){
       if(amount.value.match( /^[0-9]+(\.([0-9]+))?$/)){
             if (amount.value >= 5) {
             return true;
              }else{
                alert('Your donation must be at least $5');
                amount.focus();
                return false;
                 }
       }else{
               alert('You must enter a valid donation.');
               amount.focus();
               return false;
       }
}


    //]]>
</script>
